{
  "expo": {
    "name": "ElphantDetect",
    "slug": "ElphantDetect",
    "scheme": "ElphantDetect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/images/Logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/images/Logo.png",
      "resizeMode": "contain",
      "backgroundColor": "black"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#black"
      }
    },
    "web": {
      "favicon": "./assets/images/images/Logo.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
